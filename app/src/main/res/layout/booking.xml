<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#F5F7FA"
    >

        <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseNavbarTopOrange

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp"/>

<!--    <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseNavbarTopOrange-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="wrap_content"-->
<!--        app:layout_constraintStart_toStartOf="parent"-->
<!--        app:layout_constraintEnd_toEndOf="parent"-->
<!--        app:layout_constraintTop_toTopOf="parent"-->
<!--        />-->




    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="60dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cons1"
                android:layout_width="match_parent"
                android:layout_height="367dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@color/white"

                >




                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="335dp"
                    android:layout_height="18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="Contact Info"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:textColor="@color/Neutralblack"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="26dp"
                    />

                <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                    android:id="@+id/fullName"
                    android:layout_width="335dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/Neutraldarkgrey"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView1"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="26dp"
                    />
                <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                    android:id="@+id/phoneNumber"
                    android:layout_width="335dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/Neutraldarkgrey"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fullName"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="26dp"

                    />

                <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                    android:id="@+id/email"
                    android:layout_width="335dp"
                    android:layout_height="wrap_content"
                    android:textColor="@color/Neutraldarkgrey"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/phoneNumber"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="26dp"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cons2"
                android:layout_width="match_parent"
                android:layout_height="268dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cons1"
                android:layout_marginTop="10dp"
                android:background="@color/white"
                >


                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="335dp"
                    android:layout_height="18dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="Price"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:textColor="@color/Neutralblack"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="20dp"
                    />

                <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                    android:id="@+id/date"
                    android:layout_width="157.5dp"
                    android:layout_height="65dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="197.5dp"
                    android:layout_marginTop="58dp"
                    android:background="@color/white"
                    android:focusable="true"

                    />


                <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                    android:id="@+id/from"
                    android:layout_width="157.5dp"
                    android:layout_height="65dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="197.5dp"
                    android:layout_marginTop="173dp"
                    android:background="@color/white"
                    />
                <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                    android:id="@+id/to"
                    android:layout_width="157.5dp"
                    android:layout_height="65dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginLeft="197.5dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="173dp"
                    android:background="@color/white"
                    />



            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cons1"
                android:layout_marginTop="10dp"
                android:background="@color/white"

                >
                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/submitBooking"
                    android:layout_width="335dp"
                    android:layout_height="40dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="22dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:text="Submit"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:background="@drawable/bgndbutton"
                    />

                <ImageView
                    android:id="@+id/rectangle"
                    android:layout_width="65dp"
                    android:layout_height="6dp"
                    android:layout_marginLeft="155dp"
                    android:layout_marginRight="155dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="75dp"
                    android:background="@drawable/rectangle"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>