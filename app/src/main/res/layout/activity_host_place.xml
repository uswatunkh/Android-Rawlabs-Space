<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".rawlabspace.shared.ui.HostPlace">

    <TextView
        android:id="@+id/namaTItle"
        android:layout_width="220dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="159dp"
        android:fontFamily="@font/alike"
        android:gravity="top"
        android:text="Host"
        android:textColor="#2B2F3C"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_editor_absoluteX="20dp" />

    <ImageView
        android:id="@+id/fotoProfil"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/profileantologi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/namaorang"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namaTItle" />

    <TextView
        android:id="@+id/namaorang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="114dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/alike"
        android:gravity="top"
        android:text="Antologics"
        android:textColor="#2B2F3C"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/jamOnline"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/namaTItle" />

    <TextView
        android:id="@+id/jamOnline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="9dp"
        android:fontFamily="@font/alike"
        android:gravity="top"
        android:text="Online 2h ago"
        android:textColor="#6C748A"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintStart_toEndOf="@+id/fotoProfil" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="113.87dp"
        android:layout_height="33dp"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/bckgrndhijau"
        android:drawableLeft="@drawable/whatsapp_inc__svg"
        android:drawablePadding="5dp"
        android:fontFamily="@font/alike"
        android:gravity="start|center"
        android:lineSpacingExtra="2sp"
        android:text="WhatsApp"
        android:textAllCaps="false"
        android:textSize="15dp"
        android:translationY="-1.05sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>