<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/dialog_bg_dua"
    >

    <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseHeaderDialog
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="67dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/white"
        />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/header"
        android:layout_marginTop="70dp"
        >
       <androidx.constraintlayout.widget.ConstraintLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           >

           <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/constraint2"
               android:layout_width="match_parent"
               android:layout_height="149dp"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintTop_toTopOf="parent"
               android:background="@color/white"
               >

               <TextView
                   android:layout_width="335dp"
                   android:layout_height="18dp"
                   app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintTop_toTopOf="parent"
                   android:text="Price"
                   android:textSize="16dp"
                   android:textStyle="bold"
                   android:textColor="@color/Neutralblack"
                   android:layout_marginLeft="20dp"
                   android:layout_marginRight="20dp"
                   android:layout_marginTop="20dp"
                   />

               <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                   android:layout_width="157.5dp"
                   android:layout_height="65dp"
                   app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintTop_toTopOf="parent"
                   android:layout_marginLeft="20dp"
                   android:layout_marginRight="197.5dp"
                   android:layout_marginTop="58dp"
                   android:background="@color/white"
                   />
               <com.example.androidrawlabspace.rawlabspace.shared.ui.ReuseFormInput
                   android:layout_width="157.5dp"
                   android:layout_height="65dp"
                   app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintTop_toTopOf="parent"
                   android:layout_marginLeft="197.5dp"
                   android:layout_marginRight="20dp"
                   android:layout_marginTop="58dp"
                   android:background="@color/white"
                   />
           </androidx.constraintlayout.widget.ConstraintLayout>
           <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/constraint3"
               android:layout_width="match_parent"
               android:layout_height="149dp"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintTop_toBottomOf="@id/constraint2"
               android:layout_marginTop="10dp"
               android:background="@color/white"
               >

               <TextView
                   android:layout_width="335dp"
                   android:layout_height="18dp"
                   app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintTop_toTopOf="parent"
                   android:text="Star Rating"
                   android:textSize="16dp"
                   android:textStyle="bold"
                   android:textColor="@color/Neutralblack"
                   android:layout_marginLeft="20dp"
                   android:layout_marginRight="20dp"
                   android:layout_marginTop="20dp"
                   />


           </androidx.constraintlayout.widget.ConstraintLayout>

           <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/constraint4"
               android:layout_width="match_parent"
               android:layout_height="149dp"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintTop_toBottomOf="@id/constraint3"
               android:layout_marginTop="10dp"
               android:background="@color/white"
               >

               <TextView
                   android:layout_width="335dp"
                   android:layout_height="18dp"
                   app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintTop_toTopOf="parent"
                   android:text="Facilities"
                   android:textSize="16dp"
                   android:textStyle="bold"
                   android:textColor="@color/Neutralblack"
                   android:layout_marginLeft="20dp"
                   android:layout_marginRight="20dp"
                   android:layout_marginTop="20dp"
                   />


           </androidx.constraintlayout.widget.ConstraintLayout>
           <androidx.constraintlayout.widget.ConstraintLayout
               android:id="@+id/constraint5"
               android:layout_width="match_parent"
               android:layout_height="149dp"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintTop_toBottomOf="@id/constraint4"
               android:layout_marginTop="10dp"
               android:background="@color/white"
               >

               <TextView
                   android:layout_width="335dp"
                   android:layout_height="18dp"
                   app:layout_constraintStart_toStartOf="parent"
                   app:layout_constraintEnd_toEndOf="parent"
                   app:layout_constraintTop_toTopOf="parent"
                   android:text="Sort"
                   android:textSize="16dp"
                   android:textStyle="bold"
                   android:textColor="@color/Neutralblack"
                   android:layout_marginLeft="20dp"
                   android:layout_marginRight="20dp"
                   android:layout_marginTop="20dp"
                   />


           </androidx.constraintlayout.widget.ConstraintLayout>



       </androidx.constraintlayout.widget.ConstraintLayout>


    </ScrollView>
    




</androidx.constraintlayout.widget.ConstraintLayout>